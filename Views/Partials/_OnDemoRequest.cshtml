<script>
    $('form.ajax').on('submit', function () {
        var that = $(this),
            url = that.attr('action'),
            type = 'POST',
            data = {};
        that.find('[name]').each(function (index, value) {
            var that = $(this),
                name = that.attr('name'),
                value = that.val();

            data[name] = value;
        });
        $.ajax({
            url: url,
            type: type,
            data: data,
            success: function (response) {
                var name = document.getElementById('dlastname').value;
                var phone = document.getElementById('dPhone Number').value;
                var email = document.getElementById('demail').value;

                var result = "Thank You ";
                result = result.concat(name, '\n', 'we will contact you soon ', '\n', '           on     ', '\n', phone, ' or ', email);
                $("#output").replaceWith(result);
            }
        });
        return false;
    });
</script>

<script>
    function register_validate3() {
        var company = document.getElementById('dCompany_Name').value;
        var email = document.getElementById('demail').value;
        var name = document.getElementById('dlastname').value;
        var mobile = document.getElementById('dPhone Number').value;
        var country = document.getElementById('dcountry').value;
        var selectedDemo = document.getElementById('dProduct').value;
        var isAccepted = document.getElementById("dterms").checked;

        if (company == "") {
            document.getElementById('dCompany_Name_error').innerHTML = "Company Name Is Required";
            return false;
        }
        if (email == "") {
            document.getElementById('demail_error').innerHTML = "Email Is Required";
            return false;
        }
        if (name == "") {
            document.getElementById('dlastname_error').innerHTML = "Name Is Required";
            return false;
        }
        if (mobile == "") {
            document.getElementById('dPhone Number_error').innerHTML = "Phone Number Is Required";
            return false;
        }
        if (country == "") {
