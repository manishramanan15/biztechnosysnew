@inherits Umbraco.Web.Mvc.UmbracoViewPage
<section class="about_num">
    <div class="our_num_count">
        <div class="num_animate">
            <div class="container">
                <div class="row text-center">
                    <div class="col">
                        <div class="counter">
                            <i class="fas fa-edit fa-2x"></i>
                            <h2><span class="timer count-title count-number project" data-to="100" data-speed="3000"></span>+</h2>
                            <p class="count-text ">Project</p>
                        </div>
                    </div>
                    <div class="col">
                        <div class="counter">
                            <i class="fas fa-shield-alt fa-2x"></i>
                            <h2><span class="timer count-title count-number" data-to="99.2" data-speed="3000"></span>%</h2>
                            <p class="count-text ">Success Rate</p>
                        </div>
                    </div>
                    <div class="col">
                        <div class="counter">
                            <i class="far fa-handshake fa-2x"></i>
                            <h2><span class="timer count-title count-number" data-to="99.5" data-speed="3000"></span>%</h2>
                            <p class="count-text ">Customer Retention</p>
                        </div>
                    </div>
                    <div class="col">
                        <div class="counter">
                            <i class="fas fa-truck fa-2x"></i>
                            <h2><span class="timer count-title count-number" data-to="99.7" data-speed="3000"></span>%</h2>
                            <p class="count-text ">On-Time Delivery</p>
                        </div>
                    </div>
                    <div class="col">
                        <div class="counter">
                            <i class="fas fa-award fa-2x"></i>
                            <h2><span class="timer count-title count-number project" data-to="8" data-speed="3000"></span>+</h2>
                            <p class="count-text ">Certified</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-12 owl_num_count">
                    <div class="owl_counter owl-carousel owl-theme">
                        <div class="item">
                            <div class="col">
                                <div class="counter">
                                    <i class="fas fa-edit fa-2x"></i>
                                    <h2><span class="timer count-title count-number project" data-to="100" data-speed="3000"></span>+</h2>
                                    <p class="count-text ">Project</p>
